<% if object.errors.any? %>
  <div class='row'>
    <div class='alert-box alert radius'>
      <h5><%= pluralize(object.errors.size, "Error") %> to Fix</h5>
      <ul>
        <% object.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  </div>
<% end %>